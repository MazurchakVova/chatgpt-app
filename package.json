{
  "name": "reactron",
  "productName": "Reactron",
  "description": "Reactron - React CRA (create-react-app) with Electron.js starter kit",
  "version": "0.1.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Xgraf/reactron.git"
  },
  "author": "Dmitriy Zatulovskiy <xgrafpost@gmail.com> (https://networkroom.ru)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Xgraf/reactron/issues"
  },
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.23",
    "@types/react": "^18.0.33",
    "@types/react-dom": "^18.0.11",
    "@parcel/transformer-sass": "^2.8.3",
    "@types/downloadjs": "^1.4.3",
    "@types/lodash": "^4.14.191",
    "electron-devtools-installer": "^3.2.0",
    "electron-is-dev": "^2.0.0",
    "electron-log": "^4.4.8",
    "electron-updater": "^5.3.0",
    "node-notifier": "^10.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.10.0",
    "react-scripts": "^5.0.1",
    "reactstrap": "^9.1.8",
    "redux": "^4.2.1",
    "redux-thunk": "^2.4.2",
    "web-vitals": "^2.1.4",
    "typescript": "^4.9.5",
    "@emotion/react": "^11.10.6",
    "@emotion/server": "^11.10.0",
    "@heroicons/react": "^2.0.17",
    "@mantine/core": "^6.0.1",
    "@mantine/hooks": "^6.0.1",
    "@mantine/next": "^6.0.1",
    "@mantine/notifications": "^6.0.1",
    "@tabler/icons-react": "^2.9.0",
    "@tanstack/react-location": "^3.7.4",
    "dexie": "^3.2.3",
    "dexie-export-import": "^4.0.6",
    "dexie-react-hooks": "^1.1.3",
    "downloadjs": "^1.4.7",
    "eslint": "8.36.0",
    "eslint-config-next": "13.2.4",
    "lodash": "^4.17.21",
    "nanoid": "^4.0.1",
    "next": "13.2.4",
    "openai": "^3.2.1",
    "react-icons": "^4.8.0",
    "react-markdown": "^8.0.6",
    "remark-gfm": "^3.0.1"
  },
  "scripts": {
    "react-start": "react-scripts start",
    "react-build": "react-scripts build",
    "react-test": "react-scripts test --env=jsdom",
    "react-eject": "react-scripts eject",
    "electron-build": "electron-builder",
    "release": "yarn react-build && electron-builder --publish=always",
    "build": "yarn react-build && yarn electron-build",
    "start": "concurrently \"cross-env BROWSER=none yarn react-start\" \"wait-on http://localhost:3000 && electron .\"",
    "prettier": "prettier --single-quote --write \"./src/**/*.js\""
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "buffer": "^5.7.1",
    "parcel": "^2.8.3",
    "process": "^0.11.10",
    "concurrently": "^8.0.1",
    "cross-env": "^7.0.3",
    "electron": "^24.0.0",
    "electron-builder": "^23.6.0",
    "electron-debug": "^3.2.0",
    "prettier": "^2.8.7",
    "wait-on": "^7.0.1"
  },
  "main": "public/electron.js",
  "homepage": "./",
  "build": {
    "appId": "com.example.reactron",
    "files": [
      "build/**/*",
      "node_modules/**/*",
      "package.json"
    ],
    "directories": {
      "buildResources": "assets"
    },
    "publish": {
      "provider": "github"
    }
  }
}
